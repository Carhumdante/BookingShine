<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerInParent="false"
    android:layout_centerHorizontal="false"
    android:layout_centerVertical="false"
    android:focusableInTouchMode="false"
    android:foregroundTint="#2196F3"
    android:isScrollContainer="false"
    android:scrollbarStyle="outsideOverlay"
    android:verticalScrollbarPosition="right"
    tools:context=".LoginUserFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@color/purple_700">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="41dp"
            android:layout_marginStart="179dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="178dp"
            android:text="@string/login_usuario"
            android:textColor="#F9FBFB"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <EditText
            android:id="@+id/editTextEmailUL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="138dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="132dp"
            android:layout_marginBottom="5dp"
            android:backgroundTint="@color/design_default_color_primary"
            android:ems="10"
            android:hint="@string/email"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/editTextPasswordUL"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editTextPasswordUL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="138dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="132dp"
            android:backgroundTint="@color/design_default_color_primary"
            android:ems="10"
            android:hint="@string/Password"
            android:inputType="textPassword"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextEmailUL"
            tools:ignore="TouchTargetSizeCheck" />

        <Button
            android:id="@+id/LogUser1"
            android:layout_width="211dp"
            android:layout_height="56dp"
            android:layout_marginStart="187dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="199dp"
            android:background="@drawable/button_rounded"
            android:text="@string/login"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.418"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextPasswordUL" />

        <Button
            android:id="@+id/RegUser1"
            android:layout_width="211dp"
            android:layout_height="56dp"
            android:layout_marginStart="187dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="199dp"
            android:background="@drawable/button_rounded_border"
            android:text="@string/register"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.413"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/LogUser1" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="64dp"
            android:layout_height="75dp"
            android:layout_marginStart="121dp"
            android:layout_marginTop="41dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/button_social"
            android:scaleType="fitCenter"
            android:src="@drawable/f"
            app:layout_constraintEnd_toStartOf="@+id/imageButton4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/RegUser1"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="64dp"
            android:layout_height="72dp"
            android:layout_marginTop="44dp"
            android:layout_marginEnd="112dp"
            android:background="@drawable/button_social"
            android:scaleType="fitCenter"
            android:src="@drawable/_00221"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/RegUser1"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="168dp"
            android:layout_height="142dp"
            android:layout_marginStart="121dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="122dp"
            android:src="@drawable/logo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>