<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginEmpFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/purple_700">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="168dp"
            android:layout_height="142dp"
            android:layout_marginStart="121dp"
            android:layout_marginTop="52dp"
            android:layout_marginEnd="122dp"
            android:src="@drawable/logo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="41dp"
            android:layout_marginStart="179dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="178dp"
            android:text="@string/BusinessLogin"
            android:textColor="#F9FBFB"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView7" />

        <EditText
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="5dp"
            android:backgroundTint="@color/design_default_color_primary"
            android:ems="10"
            android:hint="Email"
            android:inputType="textPersonName"
            android:textColorHint="#8C9EFF"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editTextTextPassword2"
            android:layout_width="0dp"
            android:layout_height="52dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="32dp"
            android:backgroundTint="@color/design_default_color_primary"
            android:ems="10"
            android:hint="@string/Password"
            android:inputType="textPassword"
            android:textColorHint="#8C9EFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
            tools:ignore="TouchTargetSizeCheck" />

        <Button
            android:id="@+id/BLoginEmp"
            android:layout_width="211dp"
            android:layout_height="56dp"
            android:layout_marginStart="187dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="199dp"
            android:background="@drawable/button_rounded"
            android:text="Login"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.418"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2" />

        <Button
            android:id="@+id/BRegisterEmp"
            android:layout_width="211dp"
            android:layout_height="56dp"
            android:layout_marginStart="187dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="199dp"
            android:background="@drawable/button_rounded_border"
            android:text="Register"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.418"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/BLoginEmp" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="64dp"
            android:layout_height="75dp"
            android:layout_marginStart="121dp"
            android:layout_marginTop="41dp"
            android:layout_marginEnd="12dp"
            android:background="@drawable/button_social"
            android:scaleType="fitCenter"
            android:src="@drawable/f"
            app:layout_constraintEnd_toStartOf="@+id/imageButton4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/BRegisterEmp"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="64dp"
            android:layout_height="72dp"
            android:layout_marginTop="44dp"
            android:layout_marginEnd="112dp"
            android:background="@drawable/button_social"
            android:scaleType="fitCenter"
            android:src="@drawable/_00221"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/BRegisterEmp"
            tools:ignore="SpeakableTextPresentCheck" /><![CDATA[

        />

    ]]>
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>
